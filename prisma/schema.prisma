generator client {
  provider = "prisma-client"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Conversation {
  id         String   @id @default(cuid())
  title      String?
  rawInput   String   @db.Text
  messages   Json
  analysis   Json?
  aiProvider String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
